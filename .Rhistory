geom_text(aes(label = cm.party.ggplot$values), size = 4)+
scale_fill_manual(values = c("#DF8982", "#82DF88"))+
scale_x_discrete(position = "top")+
theme_minimal()+
theme(legend.position = "none")+
labs(title = "Confusion Matrix",
x = "Actual",
y = "Predicted")
